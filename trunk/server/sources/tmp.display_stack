void		display_stack(t_stack *execution)
{
  t_stack	*stack;
  int		i;

  i = 0;
  stack = execution;
  printf("--- Stack Begin ---\n");
  while (stack)
    {
      printf("[%d] Timestamp(%d) ", i++, (int)stack->timestamp.tv_sec);
      printf("Id Commande (%s) FD Player (%d)\n", 
	     stack->cmd, stack->fd_player);
      stack = stack->next;
    }
  printf("--- Stack End ---\n");
}